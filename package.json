    "antd-mobile": "^2.3.4",
    "axios": "^0.21.0",
    "cross-env": "^7.0.2",
    "dingtalk-jsapi": "^2.13.9",
    "echarts": "^4.9.0",
    "install": "^0.13.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.19",
    "npm": "^6.14.5",
    "prettier": "^1.19.1",
    "px2rem-loader": "^0.1.9",
    "rc-form": "^2.4.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.3",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-react": "^1.6.16"
  }
